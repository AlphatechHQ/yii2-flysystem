{
    "name": "creocoder/yii2-flysystem",
    "description": "The flysystem extension for the Yii framework",
    "keywords": [
        "yii2",
        "flysystem"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexander Kochetov",
            "email": "creocoder@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "league/flysystem": "~1.0-alpha"
    },
    "require-dev": {
        "league/flysystem-eventable-filesystem": "dev-master",
        "league/flysystem-rackspace": "dev-master",
        "league/flysystem-copy": "dev-master",
        "league/flysystem-webdav": "dev-master",
        "league/flysystem-aws-s3-v2": "dev-master",
        "league/flysystem-dropbox": "dev-master",
        "league/flysystem-cached-adapter": "dev-master",
        "league/flysystem-sftp": "dev-master",
        "league/flysystem-ziparchive": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "creocoder\\flysystem\\": "src"
        }
    }
}
